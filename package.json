    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "unplugin-icons": "^0.13.2",
    "vite": "^2.8.4",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "clsx": "^1.1.1",
